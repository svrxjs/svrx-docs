  },
  "keywords": [
    "svrx",
    "docs",
    "gitbook"
  ],
  "author": "x-orpheus",
  "license": "MIT",
  "devDependencies": {
    "gitbook-cli": "^2.3.2",
    "husky": "^3.0.0",
    "lint-md": "^0.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
